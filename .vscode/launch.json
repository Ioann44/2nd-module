{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"preLaunchTask": "Compile ts",
			"name": "Launch Program",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/dist/${relativeFileDirname}/${fileBasenameNoExtension}.js",
			"outFiles": [
				"${workspaceFolder}/dist/**/*.js"
			],
			"console": "integratedTerminal"
		}
	]
}